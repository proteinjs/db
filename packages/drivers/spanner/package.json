{
  "name": "@proteinjs/db-driver-spanner",
  "version": "1.10.6",
  "main": "./dist/generated/index.js",
  "types": "./dist/generated/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/generated/index.d.ts",
      "default": "./dist/generated/index.js"
    },
    "./test": {
      "types": "./dist/generated/test/index.d.ts",
      "default": "./dist/generated/test/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "test": [
        "dist/generated/test/index.d.ts"
      ]
    }
  },
  "description": "Db driver for Google Spanner",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proteinjs/db.git"
  },
  "bugs": {
    "url": "https://github.com/proteinjs/db/issues"
  },
  "homepage": "https://github.com/proteinjs/db#readme",
  "author": "Brent Bahry",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/ node_modules/ generated/",
    "build": "reflection-build && REFLECTION_SOURCE_DIRS=test,src REFLECTION_EXPORT_FROM=test/index.ts reflection-build && tsc",
    "watch": "reflection-watch",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@google-cloud/spanner": "7.5.0",
    "@proteinjs/db": "^1.21.1",
    "@proteinjs/db-query": "^1.4.2",
    "@proteinjs/db-spanner-common": "^1.1.22",
    "@proteinjs/db-transaction-context": "^0.2.22",
    "@proteinjs/logger": "1.0.14",
    "@proteinjs/reflection": "1.1.10",
    "@proteinjs/util": "1.5.0"
  },
  "devDependencies": {
    "@proteinjs/reflection-build": "1.4.2",
    "@types/jest": "29.5.5",
    "@types/node": "14.0.27",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
